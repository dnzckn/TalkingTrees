{
  "$schema": "1.0.0",
  "metadata": {
    "name": "Parallel Tasks",
    "version": "1.0.0",
    "author": "TalkingTrees Examples",
    "description": "Demonstrates parallel execution of multiple tasks",
    "tags": [
      "tutorial",
      "parallel",
      "concurrent"
    ]
  },
  "root": {
    "node_type": "Parallel",
    "name": "Concurrent Tasks",
    "config": {
      "policy": "SuccessOnAll"
    },
    "children": [
      {
        "node_type": "Sequence",
        "name": "Task A",
        "config": {},
        "children": [
          {
            "node_type": "Success",
            "name": "Start A",
            "config": {}
          },
          {
            "node_type": "TickCounter",
            "name": "Wait A",
            "config": {
              "duration": 10,
              "completion_status": "SUCCESS"
            }
          },
          {
            "node_type": "Success",
            "name": "Finish A",
            "config": {}
          }
        ]
      },
      {
        "node_type": "Sequence",
        "name": "Task B",
        "config": {},
        "children": [
          {
            "node_type": "Success",
            "name": "Start B",
            "config": {}
          },
          {
            "node_type": "TickCounter",
            "name": "Wait B",
            "config": {
              "duration": 5,
              "completion_status": "SUCCESS"
            }
          },
          {
            "node_type": "Success",
            "name": "Finish B",
            "config": {}
          }
        ]
      }
    ]
  },
  "dependencies": {
    "behaviors": [
      "Parallel",
      "Sequence",
      "Log",
      "Wait"
    ]
  }
}